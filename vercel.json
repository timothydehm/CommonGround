{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/create.html"
    },
    {
      "src": "/create",
      "dest": "/public/create.html"
    },
    {
      "src": "/map",
      "dest": "/public/map.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
